<!DOCTYPE html>
<html>
<head>
  <title>ZingChart &amp; Rails Gallery</title>
  <%= stylesheet_link_tag  'http://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css', 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'http://code.jquery.com/jquery-1.11.3.min.js', 'http://cdn.zingchart.com/zingchart.min.js', 'http://cdn.jsdelivr.net/ace/1.1.9/min/ace.js', 'https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.3/moment.min.js', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<header>
	<div class="container">
		<nav>
			<ul>
				<li id="home"><a href="/">ZingChart &amp; Rails</a></li>
				<li><a href="/charts">Charts</a></li>
				<li><a href="/charts/new">New</a></li>
			</ul>
		</nav>
	</div>
</header>

<section class="container">
	<%= yield %>
</section>

</body>
</html>
